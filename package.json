{
  "name": "ooniprobe-desktop",
  "description": "OONI Probe Desktop app",
  "author": "Open Observatory of Network Interference (OONI) <contact@openobservatory.org>",
  "productName": "OONI Probe",
  "version": "3.0.0-beta.4",
  "probeVersion": "3.0.0-beta.3",
  "main": "main/index.js",
  "license": "MIT",
  "repository": "ooni/probe-desktop",
  "scripts": {
    "start": "electron -r @babel/register .",
    "dev": "npm run start",
    "build": "next build renderer && next export renderer",
    "dist": "npm run build && build --dir -mw",
    "download-bin": "node scripts/download-bin.js",
    "pack": "npm run build && build -mw",
    "pack:win": "npm run build && build --win --x64",
    "pack:mac": "npm run build && build --mac",
    "pack:linux": "npm run build && build --linux",
    "icon-gen": "icon-gen -i static/Icon-Color.svg -o main/static/icons -r -m ico,icns",
    "lint": "eslint --ignore-path .gitignore .",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook -p 6006"
  },
  "build": {
    "appId": "io.ooni.probe-desktop",
    "productName": "OONI Probe",
    "asar": true,
    "files": [
      "**/*",
      "!bin",
      "!renderer",
      "renderer/out"
    ],
    "linux": {
      "target": [ "deb" ],
      "category": "Network"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "main/static/icons/app.ico",
      "extraFiles": [
        {
          "from": "./bin/windows_amd64/ooni.exe",
          "to": "./resources/bin/ooni.exe"
        },
        {
          "from": "./bin/cert.pem",
          "to": "./resources/cert.pem"
        }
      ]
    },
    "dmg": {
      "background": "build/background.png"
    },
    "mac": {
      "category": "public.app-category.tools",
      "icon": "main/static/icons/app.icns",
      "extraFiles": [
        {
          "from": "./bin/darwin_amd64/ooni",
          "to": "./Resources/bin/ooni"
        },
        {
          "from": "./bin/cert.pem",
          "to": "./Resources/cert.pem"
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/register": "7.4.4",
    "@storybook/addon-actions": "4.0.0-alpha.22",
    "@storybook/addon-links": "4.0.0-alpha.22",
    "@storybook/addon-storysource": "4.0.0-alpha.22",
    "@storybook/addons": "4.0.0-alpha.22",
    "@storybook/react": "4.0.0-alpha.22",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-source-maps": "^0.0.3",
    "aws-sdk": "^2.244.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-inline-react-svg": "1.1.0",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-styled-components": "1.10.0",
    "chroma-js": "^1.3.7",
    "csv-parse": "^2.5.0",
    "electron": "3.0.1",
    "electron-builder": "20.28.4",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.10.0",
    "humanize": "^0.0.9",
    "icon-gen": "^1.2.1",
    "moment": "^2.20.1",
    "moment-range": "^4.0.1",
    "next": "8.1.0",
    "next-progressbar": "^1.0.0",
    "ooni-components": "^0.2.7",
    "prop-types": "15.7.2",
    "raven-js": "^3.25.2",
    "rc-progress": "^2.2.5",
    "react": "16.8.6",
    "react-animations": "^0.1.0",
    "react-dom": "16.8.6",
    "react-icons": "2.2.7",
    "react-intl": "^2.4.0",
    "react-lottie": "^1.2.3",
    "remark": "^9.0.0",
    "remark-react": "^4.0.3",
    "styled-components": "3.4.10",
    "webpack": "4.29.0"
  },
  "dependencies": {
    "@sentry/electron": "^0.5.4",
    "@zeit/next-css": "^1.0.1",
    "app-root-path": "2.0.1",
    "debug": "^3.1.0",
    "debug-to-file": "^1.0.9",
    "electron-debug": "^1.5.0",
    "electron-is-dev": "1.0.1",
    "electron-next": "3.1.5",
    "electron-unhandled": "^1.1.0",
    "electron-util": "^0.8.2",
    "first-run": "^1.2.0",
    "fix-path": "^2.1.0",
    "fs-extra": "^4.0.2",
    "fstream": "^1.0.12",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "ms": "^2.0.0",
    "npm": "^6.1.0",
    "react-sticky": "^6.0.3",
    "split2": "^2.2.0",
    "sudo-prompt": "^7.1.1",
    "svgo": "^1.3.0",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "optionalDependencies": {
    "fsevents": "1.2.9"
  },
  "resolutions": {
    "js-yaml": "3.13.1",
    "rebass": "2.3.3",
    "styled-components": "3.4.10",
    "styled-system": "3.1.6",
    "react-icons": "2.2.7"
  }
}
